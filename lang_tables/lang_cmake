#include <languages.h>

bool empty_cmake_capture(char *, char *){ return true; }

TokenizerSupport cmakeSupport = {
    .comments = true,
    .strings = true,
    .numbers = true,
    .lookups = true,
    .functions = true,
    .multilineComment = false,
    .lineCommentChar = '#',
    .procs = { {"${", "}", TOKEN_ID_DATATYPE, 0, empty_cmake_capture },
               {"$<", ">", TOKEN_ID_DATATYPE, 0, empty_cmake_capture }, },
};

